<Window x:Class="matchingSheets.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:matchingSheets"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox Name="resultListBox" ItemsSource="{Binding}" HorizontalAlignment="Left" Height="268" Margin="10,28,0,0" VerticalAlignment="Top" Width="773"/>
        <local:Loader x:Name="loading" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed"></local:Loader>
    </Grid>
</Window>
